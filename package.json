  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^10.0.0",
    "hotstaq": "^0.8.56",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/formidable": "^1.2.4",
    "@types/fs-extra": "^9.0.12",
    "@types/js-cookie": "^2.2.7",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.17.32",
    "@types/node-fetch": "^2.6.1",
    "@types/selenium-webdriver": "^4.1.5",
    "@types/uuid": "^8.3.4",
    "ts-loader": "^7.0.5",
    "tslib": "^2.5.0"
